<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.soa_arah.Registration"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="249dp"
        android:layout_height="72dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:srcCompat="@mipmap/sa"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="350dp"
        android:layout_height="515dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="152dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="تسجيل"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />
    <EditText
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="اسم المستخدم(باللغة الانجليزية)"
        android:textColor="#494949"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintBottom_toBottomOf="@+id/log" />

    <TextView
        android:id="@+id/textnameempty"
        android:layout_width="209dp"
        android:layout_height="15dp"
        android:paddingLeft="15dp"
        android:paddingTop="-5dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_red_light"
        android:textSize="12sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="101dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="33dp"
        android:layout_height="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="*"
        android:textColor="@android:color/holo_red_dark"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/name"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="140dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-14dp"
        android:ems="10"
        android:gravity="right"
        android:hint="الرقم السري"
        android:inputType="textPassword"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        android:textColor="#494949"
        android:textColorHighlight="#494949"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textnameempty" />

    <TextView
        android:id="@+id/textpasswordempty"
        android:layout_width="0dp"
        android:layout_height="7dp"
        android:paddingLeft="15dp"
        android:paddingTop="-5dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="674dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="33dp"
        android:layout_height="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="*"
        android:textColor="@android:color/holo_red_dark"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.002"
        app:layout_constraintLeft_toRightOf="@+id/password"
        android:layout_marginLeft="8dp" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-4dp"
        android:ems="10"
        android:inputType="phone"
        android:hint="رقم الجوال "
        android:gravity="right"
        android:textAllCaps="false"
        android:textColor="#494949"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password" />

    <TextView
        android:id="@+id/textphoneempty"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:paddingLeft="15dp"
        android:paddingTop="-5dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="733dp" />
    <TextView
        android:id="@+id/textView2"
        android:layout_width="33dp"
        android:layout_height="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="*"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="354dp"
        app:layout_constraintLeft_toRightOf="@+id/phone"
        android:layout_marginLeft="8dp" />

    <EditText
        android:id="@+id/wedith"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="الوزن بالكجم"
        android:textColor="#494949"
        android:layout_marginTop="-3dp"
        app:layout_constraintTop_toBottomOf="@+id/phone"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />



    <EditText
        android:id="@+id/hight"
        android:layout_width="211dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="الطول بالسم"
        android:textColor="#494949"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/wedith"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.502" />

    <EditText
        android:id="@+id/Waist"
        android:layout_width="208dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="محيط الخصر بالسم "
        android:textColor="#494949"
        android:layout_marginTop="-18dp"
        app:layout_constraintTop_toBottomOf="@+id/hight"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.497" />

    <EditText
        android:id="@+id/thigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="محيط الفخذ بالسم "
        android:textColor="#494949"
        android:layout_marginTop="-10dp"
        app:layout_constraintTop_toBottomOf="@+id/Waist"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textgender"
        android:layout_width="80dp"
        android:layout_height="38dp"
        android:layout_weight="0.26"
        android:paddingTop="14dp"
        android:text="الجنس"
        android:textAlignment="textStart"
        android:textSize="18sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="#494949"
        app:layout_constraintHorizontal_bias="0.711"
        android:layout_marginTop="-23dp"
        app:layout_constraintTop_toBottomOf="@+id/thigh" />

    <Spinner
        android:id="@+id/gen"
        android:layout_width="147dp"
        android:layout_height="20dp"
        android:layout_below="@+id/textgender"
        android:layout_weight="0.58"
        android:hint="الجنس"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/thigh"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textdate"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textdate"
        android:layout_width="93dp"
        android:layout_height="44dp"
        android:layout_weight="0.26"
        android:paddingTop="14dp"
        android:text="تاريخ الميلاد"
        android:textAlignment="textStart"
        android:textSize="18sp"
        android:textColor="#494949"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textgender"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.698" />

    <TextView
        android:id="@+id/tvDate"
        android:layout_width="124dp"
        android:layout_height="29dp"
        android:layout_centerHorizontal="true"
        android:hint="اختر التاريخ"
        android:textColor="#494949"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/textdate"
        android:layout_marginRight="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="89dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="8dp"
        android:gravity="right"
        app:layout_constraintTop_toBottomOf="@+id/gen"
        app:layout_constraintVertical_bias="0.625" />

    <Button
        android:id="@+id/button"
        android:layout_width="127dp"
        android:layout_height="48dp"
        android:background="#7ba452"
        android:text="تسجيل"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textdate"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/log" />

    <TextView
        android:id="@+id/log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="هل لديك حساب؟ اضغط هنا لتسجيل الدخول"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />




</android.support.constraint.ConstraintLayout>
